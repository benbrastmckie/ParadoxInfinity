%Questions and Answers Macro

\newbool{questions}
\newbool{answers}

\newcommand{\qa}[1]{%
    \IfEqCase{#1}{%
        {q}{\booltrue{questions}\boolfalse{answers}}%
        {a}{\boolfalse{questions}\booltrue{answers}}%
        {b}{\booltrue{questions}\booltrue{answers}}%
    }[\PackageError{qa}{Undefined option to qa: #1}{}]%
}%



